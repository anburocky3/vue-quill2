  ],
  "author": "Anbuselvan Rocky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anburocky3/vue-quill2/issues"
  },
  "homepage": "https://anburocky3.github.io/vue-quill2/",
  "dependencies": {
    "quill": "^2.0.0-dev.4",
    "quill-delta": "^4.2.2"
  },
  "peerDependencies": {
    "vue": "^3.2.41"
  }
